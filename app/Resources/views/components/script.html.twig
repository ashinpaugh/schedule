{% if 'dev' == app.environment %}
    
    {% for url in urls %}
        <script type="text/javascript">
            {{ inline_resource(url)|raw }}
        </script>
    {% endfor %}
    
{% elseif 'prod' == app.environment %}
    
    <script type="text/javascript">
        {{ inline_resource(prod_url)|raw }}
    </script>
    
{% endif %}
