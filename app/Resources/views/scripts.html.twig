{##
 # Libraries.
 #}
{% javascripts
    'assets/vendor/jquery/dist/jquery.min.js'
    'assets/vendor/jquery-ui/jquery-ui.min.js'
    'assets/vendor/bootstrap/dist/js/bootstrap.js'
    'assets/vendor/moment/min/moment.min.js'
    'assets/vendor/fullcalendar/dist/fullcalendar.min.js'
    'assets/vendor/chosen/chosen.jquery.js'
    'assets/vendor/qtip2/jquery.qtip.min.js'
    'assets/vendor/spectrum/spectrum.js'
    filter='?jsqueeze'
    output='assets/compiled/js/libraries.js'
%}
    {% if 'dev' == app.environment %}
        <script>{{ inline_resource(asset_url)|raw }}</script>
    {% endif %}
{% endjavascripts %}

{% if 'dev' != app.environment %}
    <script>{{ inline_resource('assets/compiled/js/libraries.js')|raw }}</script>
{% endif %}


{##
 # App-wide utils.
 #}
{% javascripts '@ATSScheduleBundle/Resources/public/js/util/*'
    filter='?jsqueeze'
    output='assets/compiled/js/utils.js'
%}
    {% if 'dev' == app.environment %}
        <!--[if !IE]><!-->
            <script>{{ inline_resource(asset_url)|raw }}</script>
        <!--<![endif]-->
    {% endif %}
{% endjavascripts %}

{% javascripts '@ATSScheduleBundle/Resources/public/js/util/ecma5/*'
    filter='?jsqueeze'
    output='assets/compiled/js/legacy-utils.js'
%}
    {% if 'dev' == app.environment %}
        <!--[if IE]>
            <script type="text/javascript" src="{{ asset_url }}"></script>
        <![endif]-->
    {% endif %}
{% endjavascripts %}

{% if 'dev' != app.environment %}
    <!--[if !IE]><!-->
    <script>{{ inline_resource('assets/compiled/js/utils.js')|raw }}</script>
    <!--<![endif]-->
    
    <!--[if IE]>
    <script src="{{ asset('assets/compiled/js/legacy-utils.js') }}" type="text/javascript"></script>
    <![endif]-->
{% endif %}


{##
 # Page resources.
 #}
{% block javascripts %}
    {% javascripts '@ATSScheduleBundle/Resources/public/js/controller/Default/*'
        filter='?jsqueeze'
        output='assets/compiled/js/controllers.js'
    %}
        {% if 'dev' == app.environment %}
            <!--[if !IE]><!-->
                <script>{{ inline_resource(asset_url)|raw }}</script>
            <!--<![endif]-->
        {% endif %}
    {% endjavascripts %}
    
    {% javascripts '@ATSScheduleBundle/Resources/public/js/controller/Default/ecma5/*'
        filter='?jsqueeze'
        output='assets/compiled/js/legacy-controllers.js'
    %}
        {% if 'dev' == app.environment %}
            <!--[if IE]>
                <script type="text/javascript" src="{{ asset_url }}"></script>
            <![endif]-->
        {% endif %}
    {% endjavascripts %}
    
    {% if 'dev' != app.environment %}
        <!--[if !IE]><!-->
        <script>{{ inline_resource('assets/compiled/js/controllers.js')|raw }}</script>
        <!--<![endif]-->
        
        <!--[if IE]>
        <script src="{{ asset('assets/compiled/js/legacy-controllers.js') }}" type="text/javascript"></script>
        <![endif]-->
    {% endif %}
{% endblock %}